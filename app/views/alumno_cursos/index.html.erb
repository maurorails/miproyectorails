<h1>Listing alumno_cursos</h1>

<table>
  <thead>
    <tr>
      <th>Alumno</th>
      <th>Curso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumno_cursos.each do |alumno_curso| %>
      <tr>
        <td><%= alumno_curso.alumno %></td>
        <td><%= alumno_curso.curso %></td>
        <td><%= link_to 'Show', alumno_curso %></td>
        <td><%= link_to 'Edit', edit_alumno_curso_path(alumno_curso) %></td>
        <td><%= link_to 'Destroy', alumno_curso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alumno curso', new_alumno_curso_path %>
